{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Cookies and a pint",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "http://localhost/*"
    ],
    "icons": {
        "32": "primary-logo.png"
    }
}